@using Microsoft.AspNetCore.Components.Routing
@using System.Reflection
@using enuma_elish.razor.lib.Components

<Router AppAssembly="@typeof(MauiProgram).Assembly"
        AdditionalAssemblies="new[] { typeof(enuma_elish.razor.lib.Components.Main).Assembly }">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(enuma_elish.razor.lib.Shared.MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
    <NotFound>
        <MudText Typo="Typo.h6">Página no encontrada</MudText>
    </NotFound>
</Router>
